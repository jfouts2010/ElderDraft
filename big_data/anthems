er 2014\",\r\n  \"set_type\": \"commander\",\r\n  \"set_uri\": \"https://api.scryfall.com/sets/0980a6e2-eb78-4ad2-8396-cef08fce365e\",\r\n  \"set_search_uri\": \"https://api.scryfall.com/cards/search?order=set&q=e%3Ac14&unique=prints\",\r\n  \"scryfall_set_uri\": \"https://scryfall.com/sets/c14?utm_source=api\",\r\n  \"rulings_uri\": \"https://api.scryfall.com/cards/4c8ff6cd-e3ac-4024-aacb-86f7e1f43ba0/rulings\",\r\n  \"prints_search_uri\": \"https://api.scryfall.com/cards/search?order=released&q=oracleid%3A9d73a51a-1c8e-40cd-8124-aeff406a0884&unique=prints\",\r\n  \"collector_number\": \"66\",\r\n  \"digital\": false,\r\n  \"rarity\": \"uncommon\",\r\n  \"flavor_text\": \"\\\"Trust me, your lost fingers and toes are nothing compared to the lost treasures of Sejiri.\\\"\\n—Zahr Gada, Halimar expedition leader\",\r\n  \"card_back_id\": \"0aeebaf5-8c7d-4636-9e82-8c27447861f7\",\r\n  \"artist\": \"Goran Josic\",\r\n  \"artist_ids\": [\r\n    \"522bfda2-454a-4a74-b717-42930040f73a\"\r\n  ],\r\n  \"illustration_id\": \"2f9e1f1e-2540-46e2-aa62-0bd10c083bbc\",\r\n  \"border_color\": \"black\",\r\n  \"frame\": \"2015\",\r\n  \"full_art\": false,\r\n  \"textless\": false,\r\n  \"booster\": false,\r\n  \"story_spotlight\": false,\r\n  \"edhrec_rank\": 3532,\r\n  \"prices\": {\r\n    \"usd\": \"0.29\",\r\n    \"usd_foil\": null,\r\n    \"eur\": \"0.35\",\r\n    \"eur_foil\": null,\r\n    \"tix\": \"0.05\"\r\n  },\r\n  \"related_uris\": {\r\n    \"gatherer\": \"https://gatherer.wizards.com/Pages/Card/Details.aspx?multiverseid=389450\",\r\n    \"tcgplayer_decks\": \"https://decks.tcgplayer.com/magic/deck/search?contains=Brave+the+Elements&page=1&utm_campaign=affiliate&utm_medium=api&utm_source=scryfall\",\r\n    \"edhrec\": \"https://edhrec.com/route/?cc=Brave+the+Elements\",\r\n    \"mtgtop8\": \"https://mtgtop8.com/search?MD_check=1&SB_check=1&cards=Brave+the+Elements\"\r\n  },\r\n  \"purchase_uris\": {\r\n    \"tcgplayer\": \"https://shop.tcgplayer.com/product/productsearch?i