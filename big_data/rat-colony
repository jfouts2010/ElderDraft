7/mystic-sanctuary?utm_source=api\",\r\n  \"layout\": \"normal\",\r\n  \"highres_image\": true,\r\n  \"image_uris\": {\r\n    \"small\": \"https://c1.scryfall.com/file/scryfall-cards/small/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.jpg?1594662279\",\r\n    \"normal\": \"https://c1.scryfall.com/file/scryfall-cards/normal/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.jpg?1594662279\",\r\n    \"large\": \"https://c1.scryfall.com/file/scryfall-cards/large/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.jpg?1594662279\",\r\n    \"png\": \"https://c1.scryfall.com/file/scryfall-cards/png/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.png?1594662279\",\r\n    \"art_crop\": \"https://c1.scryfall.com/file/scryfall-cards/art_crop/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.jpg?1594662279\",\r\n    \"border_crop\": \"https://c1.scryfall.com/file/scryfall-cards/border_crop/front/1/7/170e792c-80d5-4775-ad95-37614574ab84.jpg?1594662279\"\r\n  },\r\n  \"mana_cost\": \"\",\r\n  \"cmc\": 0.0,\r\n  \"type_line\": \"Land — Island\",\r\n  \"oracle_text\": \"({T}: Add {U}.)\\nMystic Sanctuary enters the battlefield tapped unless you control three or more other Islands.\\nWhen Mystic Sanctuary enters the battlefield untapped, you may put target instant or sorcery card from your graveyard on top of your library.\",\r\n  \"colors\": [],\r\n  \"color_identity\": [\r\n    \"U\"\r\n  ],\r\n  \"keywords\": [],\r\n  \"produced_mana\": [\r\n    \"U\"\r\n  ],\r\n  \"legalities\": {\r\n    \"standard\": \"legal\",\r\n    \"future\": \"legal\",\r\n    \"historic\": \"legal\",\r\n    \"pioneer\": \"legal\",\r\n    \"modern\": \"legal\",\r\n    \"legacy\": \"legal\",\r\n    \"pauper\": \"banned\",\r\n    \"vintage\": \"legal\",\r\n    \"penny\": \"legal\",\r\n    \"commander\": \"legal\",\r\n    \"brawl\": \"legal\",\r\n    \"duel\": \"legal\",\r\n    \"oldschool\": \"not_legal\"\r\n  },\r\n  \"games\": [\r\n    \"arena\",\r\n    \"paper\",\r\n    \"mtgo\"\r\n  ],\r\n  \"reserved\": false,\r\n 