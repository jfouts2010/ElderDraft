 Spark\",\r\n  \"set_type\": \"expansion\",\r\n  \"set_uri\": \"https://api.scryfall.com/sets/ee044f0b-e101-4ead-8d0e-aa510aad4277\",\r\n  \"set_search_uri\": \"https://api.scryfall.com/cards/search?order=set&q=e%3Awar&unique=prints\",\r\n  \"scryfall_set_uri\": \"https://scryfall.com/sets/war?utm_source=api\",\r\n  \"rulings_uri\": \"https://api.scryfall.com/cards/d2124603-d20e-40eb-97f0-a66323397ac2/rulings\",\r\n  \"prints_search_uri\": \"https://api.scryfall.com/cards/search?order=released&q=oracleid%3A2bd111bb-ce02-414c-b5b7-e0e037d8d96b&unique=prints\",\r\n  \"collector_number\": \"79\",\r\n  \"digital\": false,\r\n  \"rarity\": \"rare\",\r\n  \"card_back_id\": \"0aeebaf5-8c7d-4636-9e82-8c27447861f7\",\r\n  \"artist\": \"Jonas De Ro\",\r\n  \"artist_ids\": [\r\n    \"561ebf9e-8d93-4b57-8156-8826d0c19601\"\r\n  ],\r\n  \"illustration_id\": \"19488800-96af-4290-921d-635fab98a891\",\r\n  \"border_color\": \"black\",\r\n  \"frame\": \"2015\",\r\n  \"frame_effects\": [\r\n    \"legendary\"\r\n  ],\r\n  \"full_art\": false,\r\n  \"textless\": false,\r\n  \"booster\": true,\r\n  \"story_spotlight\": false,\r\n  \"edhrec_rank\": 214,\r\n  \"preview\": {\r\n    \"source\": \"Wizards of the Coast\",\r\n    \"source_uri\": \"https://www.twitch.tv/videos/404095372\",\r\n    \"previewed_at\": \"2019-03-31\"\r\n  },\r\n  \"prices\": {\r\n    \"usd\": \"4.32\",\r\n    \"usd_foil\": \"5.91\",\r\n    \"eur\": \"2.25\",\r\n    \"eur_foil\": \"3.52\",\r\n    \"tix\": \"0.03\"\r\n  },\r\n  \"related_uris\": {\r\n    \"gatherer\": \"https://gatherer.wizards.com/Pages/Card/Details.aspx?multiverseid=461006\",\r\n    \"tcgplayer_decks\": \"https://decks.tcgplayer.com/magic/deck/search?contains=Bolas%27s+Citadel&page=1&utm_campaign=affiliate&utm_medium=api&utm_source=scryfall\",\r\n    \"edhrec\": \"https://edhrec.com/route/?cc=Bolas%27s+Citadel\",\r\n    \"mtgtop8\": \"https://mtgtop8.com/search?MD_check=1&SB_check=1&cards=Bolas%27s+Citadel\"\r\n  },\r\n  \"purchase_uris\": {\r\n    